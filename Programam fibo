

package fibonacci;
import java.util.Scanner;

public class Fibonacci {

   
    public static void main(String[] args) {
    
   Scanner entrada = new Scanner(System.in);
       
        int N1 = 0, N2 = 1, FI, n;
       
        System.out.print("Ingrese los numeros que quiere calcular ");
        n = entrada.nextInt();
       
        for (int i = 0; i < n; i++) {
            System.out.println(N1);
            FI = N1 + N2;
            N1 = N2;
            N2 = FI;
        }
    }
}
